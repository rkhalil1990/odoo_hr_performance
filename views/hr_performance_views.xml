	 <record id="hr_performancebonus_form" model="ir.ui.view">
        <field name="name">hr.performancebonus.form</field>
        <field name="model">hr.performancebonus</field>
        <field name="arch" type="xml">
            <form string="hr performancebonus Form">
                <sheet>
					<group>
